  </ItemGroup>
  <ItemGroup>
    <Folder Include=".vscode\" />
    <None Include="aspnetcore-https.js" />
    <None Include="babel.config.js" />
    <None Include="jsconfig.json" />
    <None Include="nuget.config" />
    <None Include="package-lock.json" />
